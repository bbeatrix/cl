ExperimentManager.seed = 1

DataFactory.target_type = 'auxiliary selfsupervised'
DataFactory.batch_size = 64
DataFactory.augment = True
DataFactory.num_tasks = 5

model_builder.model_class = @models.novelresnet18

Trainer.iters = 20000
Trainer.lr = 0.01
Trainer.optimizer = @torch.optim.SGD
Trainer.log_interval = 100

torch.optim.SGD.momentum = 0.9
torch.optim.SGD.nesterov = True
